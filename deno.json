{
  "$schema": "https://raw.githubusercontent.com/denoland/deno/main/cli/schemas/config-file.v1.json",
  "name": "@schpet/linear-cli",
  "version": "0.5.7",
  "exports": "./main.ts",
  "license": "MIT",
  "tasks": {
    "dev": "deno run '--allow-env=GITHUB_*,GH_*,LINEAR_*' --allow-read --allow-write --allow-run=git,gh,open,cmd,xdg-open,wslview --allow-net=api.linear.app --quiet main.ts ",
    "install": "deno install -c ./deno.json '--allow-env=GITHUB_*,GH_*,LINEAR_*' --allow-read --allow-write --allow-run=git,gh,open,cmd,xdg-open,wslview --allow-net=api.linear.app --quiet -g -f -n linear ./main.ts",
    "uninstall": "deno uninstall -g linear",
    "sync-schema": "deno run -A npm:get-graphql-schema https://api.linear.app/graphql > graphql/schema.graphql",
    "test": "deno test '--allow-env=GITHUB_*,GH_*,LINEAR_*' --allow-read --allow-write --allow-run=git,gh,open,cmd,xdg-open,wslview --allow-net=api.linear.app --quiet"
  },
  "imports": {
    "@cliffy/command": "jsr:@cliffy/command@^1.0.0-rc.7",
    "@cliffy/prompt": "jsr:@cliffy/prompt@^1.0.0-rc.7",
    "@littletof/charmd": "jsr:@littletof/charmd@^0.1.2",
    "@opensrc/deno-open": "jsr:@opensrc/deno-open@^1.0.0",
    "@std/assert": "jsr:@std/assert@1",
    "@std/cli": "jsr:@std/cli@^1.0.12",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.3",
    "@std/encoding": "jsr:@std/encoding@^1.0.5",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/toml": "jsr:@std/toml@^1.0.2"
  },
  "publish": {
    "include": [
      "main.ts",
      "main_test.ts",
      "config.ts",
      "README.md"
    ]
  },
  "fmt": {
    "exclude": [
      ".github/workflows/release.yml",
      ".github/workflows/build-setup.yml"
    ]
  }
}
